<!--components/infoTabs.wxml-->
<view class="tabs">
  <view class="tabs_title">
    <view 
    wx:for="{{tabs}}"
    wx:key="id" 
    class="title_item {{item.isActive?'active':''}}"
    bindtap="handleItemTap"
    data-index="{{index}}">
    {{item.name}}
    </view>
    <view class="tabs_content">
    </view> 
  </view>
</view>
